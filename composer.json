{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0.0",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "cmpayments/iban": "^1.1",
        "doctrine/dbal": "^2.6",
        "ecodenl/lvbag-php-wrapper": "^2.1",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "iio/libmergepdf": "^4.0",
        "joskolenberg/enum": "^1.2",
        "joskolenberg/laravel-jory": "^3.1",
        "laposta/laposta-api-php": "^1.5",
        "laracasts/presenter": "^0.2.1",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^10.0",
        "laravel/tinker": "~2.0",
        "laravel/ui": "^3.0",
        "league/oauth2-client": "^2.4",
        "microsoft/microsoft-graph": "^1.67",
        "mollie/laravel-mollie": "^2.14",
        "particle/validator": "^2.3",
        "php-imap/php-imap": "^4.0",
        "php-twinfield/twinfield": "2.7.*",
        "phpoffice/phpspreadsheet": "^1.7",
        "spatie/laravel-permission": "^3.0",
        "spatie/valuestore": "^1.2",
        "usmanhalalit/laracsv": "1.*@dev",
        "venturecraft/revisionable": "^1.28"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/telescope": "^4.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Databases\\Factories\\": "database/factories/",
            "Databases\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    }
}
